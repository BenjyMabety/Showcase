<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
	<table>
	<tr>
		<td colspan="2" style="font-weight:bold;">MAGIC NUMBER</td>
	</tr>
	<tr>
		<td>Guess the MAGIC number between between 0 and 100</td>
	</tr>
	<tr>
	<tr>
		<td>Value:<g:TextBox ui:field="tbMagic"></g:TextBox></td>
		<td>Attempts: <g:Label ui:field="labelAttempts"></g:Label></td>
	</tr>
	<tr>
		<td><g:Label ui:field="labelFeed"></g:Label></td>
	</tr>
	<td>
		<g:Button ui:field="enterButton"></g:Button>
	</td>
	<td colspan="5">
			<g:Button ui:field="closeButton"/>
	</td>
	</tr>
	
	</table>
	</g:HTMLPanel>
</ui:UiBinder> 